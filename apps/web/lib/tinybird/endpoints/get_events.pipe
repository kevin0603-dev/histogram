VERSION 1



NODE endpoint
SQL >
%
SELECT
    id,
    channelId,
    time,
    event,
    content,
    metadata
FROM events__v1

WHERE
    tenantId = {{ String(tenantId, required=True)}} AND
    {% if defined(channelId) %}
        channelId = {{ String(channelId, required=True) }} AND
    {% end %}
    time >= {{ Int64(since, required=True) }}
    ORDER BY time DESC
    {% if defined(limit) %}
        LIMIT {{ Int64(limit) }}
    {% end %}